bb:
  uri: "mysql+mysqlconnector://<%= scope.function_secret(["buildbot_schedulerdb_username"]) %>:<%= scope.function_secret(["buildbot_schedulerdb_password"]) %>@<%= scope.function_secret(["buildbot_schedulerdb_hostname"]) %>/<%= scope.function_secret(["buildbot_schedulerdb_database"]) %>"
bbb:
  uri: "<%= @env_config["dburi"] %>"
  poll_interval: <%= scope.lookupvar("config::buildbot_bridge2_reflector_poll_interval") %>
  reclaim-threshold: <%= scope.lookupvar("config::buildbot_bridge2_reflector_reclaim_threshold") %>
  dry-run: false
  verbose: false
  dry-run-reclaim: false
taskcluster:
  credentials:
    clientId: "<%= @env_config["client_id"] %>"
    accessToken: "<%= @env_config["access_token"] %>"
selfserve:
  api_root: "<%= @env_config["selfserve_private_url"] %>"
